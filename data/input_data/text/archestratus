   <text n="Archestrate de Géla" type="verse" xml:id="archestratus" xml:lang="grc">

      <front>

         <titlePage type="document_front">

            <pb n="Titre" xml:id="archestratus_front"/>

            <lb/>
            <lb/>

            <docTitle>
               <titlePart type="main">Archestrate de Géla, fragments</titlePart>
               <titlePart type="sub">Une édition numérique</titlePart>
            </docTitle>

            <lb/>
            <lb/>
            <lb/>
            <lb/>

            <byline>Transcription, encodage et traduction de<lb/><hi>Frédérique
               Maréchal</hi></byline>

            <lb/>
            <lb/>

            <byline>dans le cadre de son mémoire de master 1</byline>
            <byline>sous la direction d’<hi>Aurélien Berra</hi></byline>

            <lb/>
            <lb/>
            <lb/>

            <docImprint>
